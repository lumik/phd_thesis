@Article{Dhaouadi1994,
  author       = {Dhaouadi, Z. and Ghomi, M. and Mojzes, P. and Turpin, P. Y. and Chinsky, L.},
  date         = {Jun},
  journaltitle = {European Biophysics Journal with Biophysics Letters},
  title        = {VIBRATIONAL-MODE ANALYSIS OF 2-AMINOADENINE AND ITS DEUTERATED SPECIES FROM RAMAN AND ULTRAVIOLET RESONANCE RAMAN DATA},
  number       = {2},
  pages        = {95--104},
  url          = {<Go to ISI>://WOS:A1994NU20300002},
  volume       = {23},
  abstract     = {Resonance Raman spectroscopy data of 2-aminoadenine and its deuterated
	species (C8-deuterated, N-deuterated and C8-, N-deuterated derivatives)
	in aqueous solution have been collected in the spectral region between
	400 and 1800 cm(-1), by using ultraviolet excitation wavelengths
	(lambda(exc) = 222, 257 and 281 nm) located in the three main UV
	absorption bands corresponding to the strongly allowed electronic
	transitions of the molecule of interest. Moreover, a Raman spectrum
	has been recorded under off-resonance conditions with a visible excitation
	(lambda(exc) = 488 nm). In order to assign the 2-aminoadenine in-plane
	vibrational bands displayed in the RRS spectra, a normal coordinate
	analysis has been performed by means of an empirical internal valence
	force field. These calculations are based on our recent normal mode
	analysis of adenine and guanine nucleic bases and their deuterated
	species, which was based on the joint use of resonance Raman spectroscopy
	and neutron inelastic scattering data. In the 2-aminoadenine force
	field proposed here, the diagonal force constants have been directly
	transferred from those recently obtained for adenine (and from guanine
	as concerns the 2-amino group), the interaction force constants (off-diagonal)
	then being adjusted on the basis of the actual experimental data
	from 2-aminoadenine and its deuterated species. The current force
	field is also able to assign infrared and Raman data obtained by
	other authors from polycrystalline samples of the pure species.},
  file         = {1994 Dhaouadi Z et al.\; Eur. Biophys. J. 23(2) 95-104.pdf:UVRaman\\DNA\\1994 Dhaouadi Z et al.\; Eur. Biophys. J. 23(2) 95-104.pdf:PDF},
  owner        = {Lumik},
  timestamp    = {2014.02.04},
  year         = {1994},
}

@Article{Gfrorer1991,
  author       = {Gfrorer, A. and Schnetter, M. E. and Wolfrum, J. and Greulich, K. O.},
  date         = {Jul},
  journaltitle = {Berichte Der Bunsen-Gesellschaft-Physical Chemistry Chemical Physics},
  title        = {UV-RESONANCE-RAMAN STUDIES OF PROTONATED NUCLEIC-ACID BASES},
  number       = {7},
  pages        = {824--833},
  url          = {<Go to ISI>://WOS:A1991GB58500011},
  volume       = {95},
  abstract     = {UV-resonance-Raman-spectra of protonated adenine, guanine, cytosine
	and 5-methyl-cytosine (monophosphates) were taken with a UV-spectrometer
	based on an excimer laser system. Four different wavelengths (220,
	230, 260 and 270 nm) were used for resonant excitation of the Raman
	spectra and most of the spectra were taken both in H2O and in D2O.
	The very different excitation patterns obtained for different wavelengths,
	clearly demonstrate that an appropriate choice of the excitation
	wavelength for resonance enhancement increases the potential of Raman
	spectroscopy for the detection of protonated bases in complex nucleic
	acid structures significantly. For adenine the protonation is most
	clearly seen with an excitation wavelength of 220 nm, whereas for
	cytosine the most prominent changes occur at 260 nm. Apart from a
	couple of skeletal modes that undergo both frequency and intensity
	changes, the spectral region of the exocyclic modes (1550-1700 cm-1)
	yields additional information with resonant excitation. The enlarged
	spectral data basis obtained with the variation of the excitation
	wavelength is used to assign bands of the protonated species to molecular
	bond structures.},
  file         = {1991 Gfrorer A et al.\; Ber. Bunsen-Ges. Phys. Chem. (95)7 824-833.pdf:UVRaman\\DNA\\1991 Gfrorer A et al.\; Ber. Bunsen-Ges. Phys. Chem. (95)7 824-833.pdf:PDF},
