@Article{Mukerji1995,
  author    = {Mukerji, I. and Shiber, M. C. and Spiro, T. G. and Fresco, J. R.},
  title     = {POLYNUCLEOTIDES .23. A UV RESONANCE RAMAN-STUDY OF D(A(+)-G)(10), A SINGLE-STRANDED HELIX WITHOUT STACKED OR PAIRED BASES},
  journal   = {Biochemistry},
  year      = {1995},
  date      = {Nov},
  volume    = {34},
  number    = {44},
  pages     = {14300--14303},
  doi       = {10.1021/bi00044a006},
  url       = {<Go to ISI>://WOS:A1995TD84500006},
  abstract  = {UV resonance Raman spectroscopy has been utilized to directly observe
	structural features of the recently described nucleic acid single-stranded
	helix d(A(+)-G)(10). An absence of base stacking is confirmed by
	invariant hypochromic ratios of dominant vibrational modes for the
	oligomer relative to its constituent monomers as the structure is
	thermally denatured. The N-1 of dA residues is protonated, as determined
	by similarity to the ring-stretching vibrations for protonated adenine
	and its derivatives. Selective resonance enhancement of Raman vibrational
	modes from dA and dG residues shows frequency shifts upon thermal
	denaturation that confirm the participation of the exocyclic amino
	of dA but not dG residues in H-bonding. Conformationally sensitive
	glycosyl bond modes suggest anti residue conformations.},
  file      = {1995 Mukerji I et al.\; Biochemistry 34(44) 14300-14303.pdf:UVRaman\\DNA\\1995 Mukerji I et al.\; Biochemistry 34(44) 14300-14303.pdf:PDF},
  owner     = {Lumik},
  timestamp = {2012.02.06},
}

@Article{Russell1995,
  author    = {Russell, M. P. and Vohnik, S. and Thomas, G. J.},
  title     = {DESIGN AND PERFORMANCE OF AN ULTRAVIOLET RESONANCE RAMAN SPECTROMETER FOR PROTEINS AND NUCLEIC-ACIDS},
  journal   = {Biophysical Journal},
  year      = {1995},
  date      = {Apr},
  volume    = {68},
  number    = {4},
  pages     = {1607--1612},
  url       = {<Go to ISI>://WOS:A1995RD16300039},
  abstract  = {We describe an ultraviolet resonance Raman (UVRR) spectrometer appropriate
	for structural studies of biological macromolecules and their assemblies.
	instrument design includes the following features: a continuous wave,
	intracavity doubled, ultraviolet laser source for excitation of the
	Raman spectrum; a rotating cell (or jet source) for presentation
	of the sample to the laser beam; a Cassegrain optic with f/1.0 aperture
	for collection of the Raman scattering; a quartz prism dispersing
	element for rejection of stray light and Rayleigh scattering; a 0.75-m
	single grating monochromator for dispersion of the Raman scattering;
	and a liquid-nitrogen-cooled, charge-coupled device for detection
	of the Raman photons. The performance of this instrument, assessed
	on the basis of the observed signal-to-noise ratios, the apparent
	resolution of closely spaced spectral bands, and the wide spectrometer
	bandpass of 2200 cm(-1), is believed superior to previously described
	UVRR spectrometers of similar design. Performance characteristics
	of the instrument are demonstrated in UVRR spectra obtained from
	standard solvents, p-ethylphenol, which serves as a model for the
	tyrosine side chain, the DNA nucleotide deoxyguanosine-5'-monophosphate,
	and the human tumor necrosis factor binding protein, which is considered
	representative of soluble globular proteins.},
  file      = {1995 Russell MP et al.\; Biophys. J. 68 1607-1612.pdf:UVRaman\\DNA\\Popis spektráku\\1995 Russell MP et al.\; Biophys. J. 68 1607-1612.pdf:PDF},
